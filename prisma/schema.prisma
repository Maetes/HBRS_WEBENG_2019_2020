generator client {
  provider = "prisma-client-js"
}

datasource mysql {
  provider = process.env.DB
  url      = process.env.DB_CONNECTION
}

model Project {
  beschreibung String?
  click        String
  id           Int     @default(autoincrement()) @id
  name         String  @unique
  url          String
}

model Task {
  beschreibung String
  click        String
  code         String
  id           Int    @default(autoincrement()) @id
  name         String @unique
}

model Tech {
  beschreibung String?
  id           Int     @default(autoincrement()) @id
  name         String  @unique
  pic          String
}

model User {
  email    String  @unique
  id       Int     @default(autoincrement()) @id
  nachname String
  password String
  title    String?
}
